{
  "rules": {
    "indent": 0,
    "max-len": 0,
    "no-useless-concat": 0,
    "one-var": 0,
    "no-var": 0,
    "space-before-function-paren": 0,
    "require-jsdoc": 0,
    "no-eq-null": 0,
    "eqeqeq": ["error", "allow-null"],
    "no-else-return": 0,
    "no-negated-condition": 0,
    "camelcase": 0,
    "no-lonely-if": 0,
    "no-implicit-coercion": 0,
    "wrap-iife": ["error", "any"],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "max-statements-per-line": 0,
    "new-cap": 0,
    "object-curly-spacing": 0,
    "no-nested-ternary": 0,
    "comma-dangle": ["error", "never"],
    "prefer-spread": 0,
    "arrow-parens": ["error", "as-needed"],
    "no-undef": "error",
    "no-invalid-this": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "worker": true,
    "amd": true,
    "jasmine": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "inject": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "google"
}
